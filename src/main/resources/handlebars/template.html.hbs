<html>
<head>
    <title>Ratpack01</title>
</head>
<body>
<h1>Ratpack using Handlebars</h1>

<p>Account Manager: {{firstName}} {{lastName}}</p>

<table>
    <thead>
    <tr>
        <th>Name</th>
        <th>Active</th>
        <th>Start Date</th>
    </tr>
    </thead>
    {{#each campaigns}}
        <tr>
            <td>{{name}}</td>
            <td>{{active}}</td>
            <td>{{startDate}}</td>
        </tr>
    {{/each}}
</table>
</body>
</html>